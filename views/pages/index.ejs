<% if (currentUser !== undefined) { %>
<script>
    var currentUser = {
        id: '<%= currentUser.id %>',
        username: '<%= currentUser.username %>',
        role: '<%= currentUser.role %>',
        email: '<%= currentUser.email %>',
        first_name: '<%= currentUser.first_name %>',
        no: '<%= currentUser.no %>',
        createdAt: '<%= currentUser.createdAt %>',
        updatedAt: '<%= currentUser.updatedAt %>'

    };
</script>
<% } %>




<div class="container" ui-view="main">
    <% if (lastLectures !== undefined) { %>
    <h4>Последнии добавленные лекции</h4>
    <ul class="list-group">
        <% lastLectures.forEach(function(item){ %>
        <li  class="list-group-item"><a href="/lectures/<%= item.title %>?slideshareId=<%= item.slideshareId %>"> <%= item.title %></a> <span class="badge"> <%= item.slides.length %> слайд(ов) </span></li>
        <% }); %>
    </ul>
    <% } %>
</div><!-- /.container -->